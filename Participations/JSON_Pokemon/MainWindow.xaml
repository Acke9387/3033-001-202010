        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:JSON_Pokemon"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <ComboBox x:Name="cboPokies" HorizontalAlignment="Left" Margin="93,75,0,0" VerticalAlignment="Top" Width="120" SelectionChanged="cboPokies_SelectionChanged"/>
        <Image x:Name="imgPokemon" HorizontalAlignment="Left" Height="198" VerticalAlignment="Top" Width="346" Margin="208,174,0,0"/>
        <Label x:Name="lblStuff" Content="Label" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="156,141,0,0"/>

    </Grid>
</Window>
